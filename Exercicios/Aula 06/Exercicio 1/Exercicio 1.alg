algoritmo "Aula 06"
// Função :Aula 06
// Autor :João Emanuel
// Data : 11/04/2021
// Seção de Declarações 
var
   dolar, reeal, soma: real
inicio
// Seção de Comandos 
   Escreval ("Eu tenho quantos reais?")
   Leia(reeal)
   dolar <- 5.69
   soma <- (reeal * dolar)
   Escreval (reeal," Reais em Dolares vale $", soma," Dolares")
fimalgoritmo
