// Seção de Declarações
var
c1, c2, c3, c: inteiro
procedimento sequencia(var a, b: inteiro)
var c: inteiro
inicio
c <- a + b
escreva(c)
a <- b
b <- c
fimprocedimento
inicio
escreva(c1)
c2 <- 1
escreva(c2)
// Seção de Comandos
para c <- 3 ate 10 faca
   c3 <- c1 + c2
   sequencia(c1, c2)
fimpara
fimalgoritmo
