algoritmo "Aula 09"
// Função : Aula 09
// Autor : João Emanuel
// Data : 13/04/2021
// Seção de Declarações
var
c, s: real
l: inteiro
inicio
// Seção de Comandos
l <- 0
enquanto (l <= 9) faca
   escreval("Digite um numero")
   leia(c)
   s <- s + c
   l <- l + 1
fimenquanto
Escreval("A soma entre os 10 numeros digitados é igual a",s)
fimalgoritmo
