algoritmo "Aula 14"
// Função : Aula 14
// Autor : João Emanuel
// Data : 15/04/2021
// Seção de Declarações
var
v: vetor[1..4] de inteiro
c: inteiro
inicio
// Seção de Comandos
para c <- 1 ate 4 faca
   escreva("Digite um valor ")
   leia(v[c])
fimpara
para c <- 1 ate 4 faca
   escreva("[", v[c], " ]")
fimpara
fimalgoritmo
